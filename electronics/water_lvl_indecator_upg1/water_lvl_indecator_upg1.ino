void setup() {
  // initialize digital pin LED_BUILTIN as an output.
  pinMode(LED_BUILTIN, OUTPUT);
  Serial.begin(115200);
  pinMode(trig, OUTPUT);
  pinMode(echo, INPUT);

  // SSD1306_SWITCHCAPVCC = generate display voltage from 3.3V internally
  Serial.begin(115200);

  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;);
  }
  delay(2000); // Pause for 2 seconds

  // Clear the buffer.
  display.clearDisplay();

  // Draw bitmap on the screen
  display.drawBitmap(0, 0, image_data_Untitled, 128, 64, 1);
  display.display();
  delay(5000);
  display.clearDisplay();
  display.drawBitmap(0, 0, image_data_Untitleood, 128, 64, 1);
  display.display();
  delay(5000);
  display.clearDisplay();
  display.drawBitmap(0, 0, image_data_Untitleod, 128, 64, 1);
  display.display();
  delay(5000);

  //testdrawstyles();    // Draw 'stylized' characters

}

void loop() {

  int t = 0, h = 0, hp = 0, n = 0;

  // Transmitting pulse
  digitalWrite(trig, LOW);
  delayMicroseconds(2);
  digitalWrite(trig, HIGH);
  delayMicroseconds(200);
  digitalWrite(trig, LOW);

  // Waiting for pulse
  t = pulseIn(echo, HIGH);


  // Calculating distance
  n = t / 58;

  n = n - 24;  // offset correction
  n = 17.50 - n;  // water height, 0 - 50 cm

  hp = 5.88 * n; // distance in %, 0-100 %

  // Sending to computer
  Serial.print(hp);
  Serial.print(t);
  // Serial.print(" cm\n");
  Serial.print("\n");

  delay(100);

  testdrawstyles(hp);
  aninani(hp);


}
