  display.setCursor(0, 0);            // Start at top-left corner
  display.println(F("waterlevel"));
  display.setTextSize(2);             // Draw 2X-scale text
  display.setTextColor(WHITE);
  display.print("=");
  display.print(a);
  display.setTextSize(2);             // Draw 2X-scale text
  display.setTextColor(WHITE);
  display.print(F("%"));
  display.display();
  delay(1000);
}

